{
  "name": "nextauth",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "scripts": {
    "go": "concurrently \"pnpm run server\" \"pnpm dev\"",
    "server": "json-server -w server/db.json -p 3001",
    "dev": "prisma generate && next dev",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "next lint",
    "autofix": "pnpm eslint --fix \"./**/*.{js,jsx,ts,tsx*}\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@nanostores/query": "^0.1.3",
    "@nanostores/react": "^0.7.1",
    "@prisma/client": "^4.15.0",
    "@types/node": "20.2.6",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "nanostores": "^0.9.2",
    "next": "13.4.4",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "json-server": "^0.17.3",
    "prisma": "^4.15.0"
  }
}
